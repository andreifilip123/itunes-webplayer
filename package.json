{
  "name": "itunes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "axios": "^0.21.1",
    "core-js": "^2.5.7",
    "foundation-sites": "^6.5.0",
    "jquery": "^3.5.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "cache-loader": "^1.2.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "comma-dangle": 0,
      "space-before-function-paren": 0,
      "global-require": 0,
      "prefer-template": 0,
      "no-console": 0,
      "arrow-parens": 0,
      "object-shorthand": 0,
      "no-param-reassign": 0,
      "import/prefer-default-export": 0,
      "eqeqeq": 0,
      "max-len": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
